<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="#FFE082" /> <!-- 누르면 노란빛 하이라이트 -->
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="@android:color/transparent" /> <!-- 기본은 투명 -->
        </shape>
    </item>
</selector>
